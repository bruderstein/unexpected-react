{
  "name": "unexpected-react",
  "version": "3.2.3",
  "description": "Plugin for unexpected, to allow for assertions on the React.js virtual DOM, and the shallow renderer",
  "scripts": {
    "build": "babel src -d lib",
    "prepublish": "npm run build && npm test",
    "test-main": "mocha --compilers js:babel-core/register src/tests/*.spec.js",
    "test-error": "mocha --compilers js:babel-core/register src/error-tests/*.spec.js",
    "test-documentation": "mocha --compilers js:babel-core/register,md:unexpected-markdown --require ./bootstrap-unexpected-markdown --recursive documentation",
    "lint": "eslint src",
    "generate-site": "generate-site --require ./bootstrap-unexpected-markdown.js",
    "deploy-site": "deploy-site.sh",
    "test": "npm run test-main && npm run test-error",
    "coveralls": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- --compilers js:babel-core/register src/tests/*.spec.js &&cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage || true"
  },
  "main": "lib/unexpected-react.js",
  "repository": "https://github.com/bruderstein/unexpected-react.git",
  "license": "MIT",
  "keywords": [
    "unexpected-plugin",
    "unexpected",
    "testing",
    "react",
    "reactjs",
    "unit-test",
    "virtualDOM",
    "test"
  ],
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0",
    "react-addons-test-utils": "^0.14.0 || ^15.0.0",
    "react-dom": "^0.14.0 || ^15.0.0"
  },
  "devDependencies": {
    "babel-cli": "6.10.1",
    "babel-core": "6.9.1",
    "babel-eslint": "^6.0.0",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.5.0",
    "coveralls": "^2.11.9",
    "eslint": "^2.2.0",
    "eslint-plugin-react": "^3.8.0",
    "immutable": "^3.7.6",
    "istanbul": "^0.4.3",
    "jsdom": "^6.5.1",
    "magicpen": "^5.7.0",
    "mocha": "^2.3.3",
    "mocha-lcov-reporter": "^1.2.0",
    "object-assign": "^4.0.1",
    "react": "^15.0.0",
    "react-addons-test-utils": "^15.0.0",
    "react-dom": "^15.0.0",
    "unexpected": "^10.13.2",
    "unexpected-documentation-site-generator": "^4.1.0",
    "unexpected-markdown": "^1.7.1"
  },
  "dependencies": {
    "magicpen-prism": "^2.2.1",
    "react-render-hook": "^0.1.3",
    "unexpected-htmllike": "^2.1.1",
    "unexpected-htmllike-jsx-adapter": "^1.0.0",
    "unexpected-htmllike-reactrendered-adapter": "^2.0.1"
  }
}
